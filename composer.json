{
    "name": "ahmedkandel/nova-checklist",
    "description": "A Laravel Nova field to display a list of items to complete.",
    "keywords": [
        "laravel",
        "nova",
        "checklist",
        "checklists"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Ahmed Kandel"
    }],
    "require": {
        "laravel/nova": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Ahmedkandel\\NovaChecklist\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ahmedkandel\\NovaChecklist\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
